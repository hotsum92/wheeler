{
  "name": "chrome-extension-sample",
  "version": "0.0.1",
  "scripts": {
    "build": "webpack --mode production",
    "dev:build": "webpack --mode production --env mode=development",
    "dev:content": "webpack-dev-server --mode development --env entry=./src/content.tsx",
    "dev:options": "webpack-dev-server --mode development --env entry=./src/options.tsx",
    "type": "tsc --noEmit",
    "test": "jest --verbose",
    "test:watch": "npm test -- --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-interactions": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/builder-webpack5": "^6.4.19",
    "@storybook/manager-webpack5": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@storybook/testing-library": "^0.0.9",
    "@types/chrome": "^0.0.180",
    "@types/jest": "^27.4.1",
    "@types/react-dom": "^17.0.14",
    "@types/redux-logger": "^3.0.9",
    "copy-webpack-plugin": "^10.2.4",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.5.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
    "@mui/material": "^5.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.9",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3"
  }
}
